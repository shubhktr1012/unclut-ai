# Gmail Unsubscriber Configuration

# Maximum number of senders to process at once
MAX_SENDERS=50

# Maximum number of emails to scan for unsubscribe links
MAX_EMAILS_TO_SCAN=100

# If true, performs a dry run without actually unsubscribing or deleting
DRY_RUN=false

# Gmail user ID (usually 'me' for the authenticated user)
USER_ID=me
